<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="cordova-plugin-localizations-plist"
        version="1.0.0">

    <name>cordova-plugin-localizations-plist</name>
    <description>Cordova/PhoneGap plugin for set your localizations in Info.plist</description>
    <license>Apache 2.0 License</license>
    <platform name="ios">
        <config-file target="*-Info.plist" parent="CFBundleDevelopmentRegion">
            <array>
                <string>French</string>
            </array>
        </config-file>
        <config-file target="*-Info.plist" parent="CFBundleLocalizations">
            <array>
                <string>fr</string>
            </array>
        </config-file>
    </platform>
</plugin>
